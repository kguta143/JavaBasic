import java.util.Calendar;
import java.util.Scanner;

public class First {

	public static void main(String[] args) {

		/*
		2. 1부터 100까지 숫자 중 3의 배수 이거나 5의 배수인 숫자의 합을 구하는 
   			소스코드와 결과화면 스크린 샷을 제출하시오
    		[요구사항] 
   			가. 단, 3와 5의 공배수인 경우 제외
   			나, while문 버전과  for문 버전으로 두가지 소스코드를 작성하시오
		*/
		Scanner sc = new Scanner(System.in);
		System.out.print("숫자 하나를 입력해주세요 : ");
		int su1 = sc.nextInt();

		while(su1 % 3 != 0 && su1 % 5 != 0) {
		System.out.print("숫자 하나를 입력해주세요 : ");
		su1 = sc.nextInt();
		}

		if(su1 % 3 == 0 && su1 % 5 ==0) {
		System.out.println("3과 5의 공배수");
		}
		else if(su1 % 3 == 0) {
		System.out.println("3의 배수");
		}
		else if(su1 % 5 == 0) {
		System.out.println("5의 배수");
		}
				
		
	}

}
