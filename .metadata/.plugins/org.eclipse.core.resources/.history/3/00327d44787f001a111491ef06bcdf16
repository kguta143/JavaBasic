package Test;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		for(int s=0; s<3; s++) {
			for(int i=1; i<10; i++) {
				System.out.println(a+" * "+i+" = "+a*i);
				System.out.println(b+" * "+i+" = "+b*i);
			}
		}
	}
}
