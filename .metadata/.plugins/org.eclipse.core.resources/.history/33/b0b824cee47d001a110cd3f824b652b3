package Test;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {

		
		int a=0;
		int even=0;
		int odd=0;
		Scanner sc = new Scanner(System.in);
		
		a=sc.nextInt();
		
		for(int i=0; i<a; i++) {
			if(a%2==0) {
				even++;
			}
			else {
				odd++;
			}
		}
		System.out.println("even : "+even);
		System.out.println("odd : "+odd);
	}

}
